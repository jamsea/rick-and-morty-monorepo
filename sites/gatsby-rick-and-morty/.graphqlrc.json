{
  "schema": "https://localhost:8000/___graphql",
  "documents": [
    "./src/**/*.{graphql,js,ts,jsx,tsx}",
    "./node_modules/gatsby*/!(node_modules)/**/*.js"
  ],
  "extensions": {
    "endpoints": {
      "default": {
        "url": "https://localhost:8000/___graphql"
      }
    },
    "codegen": [
      {
        "generator": "graphql-binding",
        "language": "typescript",
        "output": {
          "binding": "src/graphqlTypes.ts"
        }
      }
    ]
  }
}
