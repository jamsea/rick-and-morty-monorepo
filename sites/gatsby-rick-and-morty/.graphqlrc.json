{
  "schema": "http://localhost:8000/___graphql",
  "documents": ["src/**/*.{graphql,js,ts,jsx,tsx}"],
  "extensions": {
    "codegen": [
      {
        "generator": "graphql-binding",
        "language": "typescript",
        "output": {
          "binding": "generated/db.ts"
        }
      }
    ]
  }
}
