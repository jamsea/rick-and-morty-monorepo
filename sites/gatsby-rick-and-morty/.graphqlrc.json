{
  "schema": "http://localhost:8000/___graphql",
  "documents": [
    "./src/**/*.{graphql,js,ts,jsx,tsx}",
    "./node_modules/gatsby*/!(node_modules)/**/*.js"
  ],
  "generates": {
    "./src/graphqlTypes.ts": {
      "plugins": ["typescript", "typescript-operations"]
    }
  },
  "extensions": {
    "codegen": [
      {
        "generator": "graphql-binding",
        "language": "typescript",
        "output": {
          "binding": "src/graphqlTypes.ts"
        }
      }
    ]
  }
}
